{
  "dashboard": {
    "title": "Lex-PR Runner - Production Monitoring",
    "tags": ["lex-pr-runner", "production", "monitoring"],
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "Plan Execution Time (p50, p95, p99)",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.50, rate(lex_pr_plan_execution_seconds_bucket[5m]))",
            "legendFormat": "p50"
          },
          {
            "expr": "histogram_quantile(0.95, rate(lex_pr_plan_execution_seconds_bucket[5m]))",
            "legendFormat": "p95"
          },
          {
            "expr": "histogram_quantile(0.99, rate(lex_pr_plan_execution_seconds_bucket[5m]))",
            "legendFormat": "p99"
          }
        ],
        "gridPos": {
          "x": 0,
          "y": 0,
          "w": 12,
          "h": 8
        }
      },
      {
        "id": 2,
        "title": "Gate Success Rate by Type",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(lex_pr_gate_success_total[5m]) / (rate(lex_pr_gate_success_total[5m]) + rate(lex_pr_gate_failure_total[5m]))",
            "legendFormat": "{{gateType}}"
          }
        ],
        "gridPos": {
          "x": 12,
          "y": 0,
          "w": 12,
          "h": 8
        }
      },
      {
        "id": 3,
        "title": "Merge Success Rate",
        "type": "stat",
        "targets": [
          {
            "expr": "rate(lex_pr_merge_success_total[5m]) / (rate(lex_pr_merge_success_total[5m]) + rate(lex_pr_merge_failure_total[5m]))",
            "legendFormat": "Success Rate"
          }
        ],
        "gridPos": {
          "x": 0,
          "y": 8,
          "w": 6,
          "h": 4
        }
      },
      {
        "id": 4,
        "title": "Memory Usage",
        "type": "graph",
        "targets": [
          {
            "expr": "lex_pr_memory_usage_bytes{type=\"heap_used\"}",
            "legendFormat": "Heap Used"
          }
        ],
        "gridPos": {
          "x": 6,
          "y": 8,
          "w": 6,
          "h": 4
        }
      },
      {
        "id": 5,
        "title": "Active Workers",
        "type": "stat",
        "targets": [
          {
            "expr": "lex_pr_active_workers",
            "legendFormat": "Workers"
          }
        ],
        "gridPos": {
          "x": 12,
          "y": 8,
          "w": 6,
          "h": 4
        }
      },
      {
        "id": 6,
        "title": "Dependency Resolution Time",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(lex_pr_dependency_resolution_seconds_bucket[5m]))",
            "legendFormat": "p95"
          }
        ],
        "gridPos": {
          "x": 18,
          "y": 8,
          "w": 6,
          "h": 4
        }
      },
      {
        "id": 7,
        "title": "Gate Execution Time by Type",
        "type": "heatmap",
        "targets": [
          {
            "expr": "rate(lex_pr_gate_execution_seconds_bucket[5m])",
            "legendFormat": "{{gateType}}"
          }
        ],
        "gridPos": {
          "x": 0,
          "y": 12,
          "w": 24,
          "h": 8
        }
      },
      {
        "id": 8,
        "title": "Error Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(lex_pr_gate_failure_total[5m])",
            "legendFormat": "{{gateType}}"
          }
        ],
        "gridPos": {
          "x": 0,
          "y": 20,
          "w": 12,
          "h": 8
        }
      },
      {
        "id": 9,
        "title": "Dependency Resolution Accuracy",
        "type": "stat",
        "targets": [
          {
            "expr": "lex_pr_dependency_accuracy_ratio",
            "legendFormat": "Accuracy"
          }
        ],
        "gridPos": {
          "x": 12,
          "y": 20,
          "w": 12,
          "h": 8
        }
      }
    ],
    "schemaVersion": 27,
    "version": 1
  }
}
